                    [
                        "all",
                        ["==", ["get", "road_kind"], "highway"],
                        ["!=", ["get", "road_kind_detail"], "trunk"]
                    ],
                    [
                        "world-ppi-scale",
                        [
                            "+",
                            ["*", 2, ["ref", "TrafficStyle.NoRoute.Category0.OutlineWidth"]],
                            ["ref", "TrafficStyle.NoRoute.Category0.Width"]
