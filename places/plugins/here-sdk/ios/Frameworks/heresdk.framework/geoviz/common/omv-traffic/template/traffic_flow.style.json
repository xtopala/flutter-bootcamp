                            ["ref", "TrafficStyle.NoRoute.Category3.Width"]
                        ]
                    ],
                    [
                        "all",
                        ["==", ["get", "road_kind"], "major_road"],
                        ["==", ["get", "road_kind_detail"], "secondary"]
                    ],
                    [
                        "world-ppi-scale",
                        [
