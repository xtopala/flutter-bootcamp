                "antialias": [
                    "case",
                    [
                        "any",
                        ["==", ["get", "road_kind"], "minor_road"],
                        [
                            "all",
                            ["==", ["get", "road_kind"], "major_road"],
                            ["==", ["get", "road_kind_detail"], "tertiary"]
                        ],
                        [
                            "all",
                            ["==", ["get", "road_kind"], "major_road"],
                            ["==", ["get", "road_kind_detail"], "secondary"]
