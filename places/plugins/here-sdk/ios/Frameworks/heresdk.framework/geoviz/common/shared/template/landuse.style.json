{
    "styles": [
        {
            "id": "landuse_points",
            "styleSet": "tilezen_points",
            "when": ["==", ["get", "$layer"], "landuse"],
            "technique": "text",
            "category": "landuse-labels",
            "attr": {
                "margin": ["ref", "defaultMargin"],
                "persisting-margin-reducing": ["ref", "defaultPersistingMarginReducing"],
                "far-threshold-range": 0.015,
                "fade-time": 0.3,
                "outline-style": "OutlineAndFill",
                "break-line": true,
                "outline-size": ["match", ["get", "kind"],
                    "low_emission_zone", ["ppi-scale", ["ref", "CongestionZoneInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    ["national_park", "nature_reserve"], ["ppi-scale", ["ref", "NationalParkInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "forest", ["ppi-scale", ["ref", "WoodlandInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "theme_park", ["ppi-scale", ["ref", "AmusementParkInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "zoo", ["ppi-scale", ["ref", "AnimalParkInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "cemetery", ["ppi-scale", ["ref", "CemeteryInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "hospital", ["ppi-scale", ["ref", "HospitalCampusInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "park", ["ppi-scale", ["ref", "ParkInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "pedestrian", ["ppi-scale", ["ref", "PedestrianAreaInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    ["aerodrome", "airfield"], ["ppi-scale", ["ref", "AirportAreaInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "harbour", ["ppi-scale", ["ref", "HarborAreaInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "industrial", ["ppi-scale", ["ref", "IndustrialComplexInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "glacier", ["ppi-scale", ["ref", "LandCover.Glacier.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "water_park", ["ppi-scale", ["ref", "WaterParkInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "railway", ["ppi-scale", ["ref", "RailyardInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "golf_course", ["ppi-scale", ["ref", "GolfCourseInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "reservation", ["ppi-scale", ["ref", "NativeReservationInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "military", ["ppi-scale", ["ref", "MilitaryBaseInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "allotments", ["ppi-scale", ["ref", "AllotmentInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "sports_centre", ["ppi-scale", ["ref", "SportsComplexInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "commercial", ["ppi-scale", ["ref", "ShoppingComplexInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "beach", ["ppi-scale", ["ref", "BeachInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    "university", ["ppi-scale", ["ref", "UniversityCampusInfo.FontStyle.OutlineWidth"], ["ref", "ScalePpi.labels"]],
                    ["ppi-scale", 1, ["ref", "ScalePpi.labels"]]],
                "replaceable": true,
                "new-label-spread-frames": 10,
                "leading-space": ["match", ["get", "kind"],
                    ["national_park", "nature_reserve"], ["ref", "NationalParkInfo.FontStyle.Leading"],
                    ["ref", "AreaInfo.FontStyle.Leading"]],
                "tracking-space": ["match", ["get", "kind"],
                    ["national_park", "nature_reserve"], ["ref", "NationalParkInfo.FontStyle.Tracking"],
                    null],
                "outline-color": ["match", ["get", "kind"],
                    "low_emission_zone", ["ref", "CongestionZoneInfo.FontStyle.OutlineColor"],
                    ["national_park", "nature_reserve"], ["ref", "NationalParkInfo.FontStyle.OutlineColor"],
                    "forest", ["ref", "WoodlandInfo.FontStyle.OutlineColor"],
                    "theme_park", ["ref", "AmusementParkInfo.FontStyle.OutlineColor"],
                    "zoo", ["ref", "AnimalParkInfo.FontStyle.OutlineColor"],
                    "cemetery", ["ref", "CemeteryInfo.FontStyle.OutlineColor"],
                    "hospital", ["ref", "HospitalCampusInfo.FontStyle.OutlineColor"],
                    "park", ["ref", "ParkInfo.FontStyle.OutlineColor"],
                    "pedestrian", ["ref", "PedestrianAreaInfo.FontStyle.OutlineColor"],
                    ["aerodrome", "airfield"], ["ref", "AirportAreaInfo.FontStyle.OutlineColor"],
                    "harbour", ["ref", "HarborAreaInfo.FontStyle.OutlineColor"],
                    "industrial", ["ref", "IndustrialComplexInfo.FontStyle.OutlineColor"],
                    "glacier", ["ref", "LandCover.Glacier.FontStyle.OutlineColor"],
                    "water_park", ["ref", "WaterParkInfo.FontStyle.OutlineColor"],
                    "railway", ["ref", "RailyardInfo.FontStyle.OutlineColor"],
                    "golf_course", ["ref", "GolfCourseInfo.FontStyle.OutlineColor"],
                    "reservation", ["ref", "NativeReservationInfo.FontStyle.OutlineColor"],
                    "military", ["ref", "MilitaryBaseInfo.FontStyle.OutlineColor"],
                    "allotments", ["ref", "AllotmentInfo.FontStyle.OutlineColor"],
                    "sports_centre", ["ref", "SportsComplexInfo.FontStyle.OutlineColor"],
                    "commercial", ["ref", "ShoppingComplexInfo.FontStyle.OutlineColor"],
                    "beach", ["ref", "BeachInfo.FontStyle.OutlineColor"],
                    "university", ["ref", "UniversityCampusInfo.FontStyle.OutlineColor"],
                    "#008000"],
                "far-threshold": 0.85,
                "color": ["match", ["get", "kind"],
                    "low_emission_zone", ["ref", "CongestionZoneInfo.FontStyle.Color"],
                    ["national_park", "nature_reserve"], ["ref", "NationalParkInfo.FontStyle.Color"],
                    "forest", ["ref", "WoodlandInfo.FontStyle.Color"],
                    "theme_park", ["ref", "AmusementParkInfo.FontStyle.Color"],
                    "zoo", ["ref", "AnimalParkInfo.FontStyle.Color"],
                    "cemetery", ["ref", "CemeteryInfo.FontStyle.Color"],
                    "hospital", ["ref", "HospitalCampusInfo.FontStyle.Color"],
                    "park", ["ref", "ParkInfo.FontStyle.Color"],
                    "pedestrian", ["ref", "PedestrianAreaInfo.FontStyle.Color"],
                    ["aerodrome", "airfield"], ["ref", "AirportAreaInfo.FontStyle.Color"],
                    "harbour", ["ref", "HarborAreaInfo.FontStyle.Color"],
                    "industrial", ["ref", "IndustrialComplexInfo.FontStyle.Color"],
                    "glacier", ["ref", "LandCover.Glacier.FontStyle.Color"],
                    "water_park", ["ref", "WaterParkInfo.FontStyle.Color"],
                    "railway", ["ref", "RailyardInfo.FontStyle.Color"],
                    "golf_course", ["ref", "GolfCourseInfo.FontStyle.Color"],
                    "reservation", ["ref", "NativeReservationInfo.FontStyle.Color"],
                    "military", ["ref", "MilitaryBaseInfo.FontStyle.Color"],
                    "allotments", ["ref", "AllotmentInfo.FontStyle.Color"],
                    "sports_centre", ["ref", "SportsComplexInfo.FontStyle.Color"],
                    "commercial", ["ref", "ShoppingComplexInfo.FontStyle.Color"],
                    "beach", ["ref", "BeachInfo.FontStyle.Color"],
                    "university", ["ref", "UniversityCampusInfo.FontStyle.Color"],
                    "#000000"],
                "size": ["match", ["get", "kind"],
                    "low_emission_zone", ["ppi-scale", ["ref", "CongestionZoneInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    ["national_park", "nature_reserve"], ["ppi-scale", ["ref", "NationalParkInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "forest", ["ppi-scale", ["ref", "WoodlandInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "theme_park", ["ppi-scale", ["ref", "AmusementParkInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "zoo", ["ppi-scale", ["ref", "AnimalParkInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "cemetery", ["ppi-scale", ["ref", "CemeteryInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "hospital", ["ppi-scale", ["ref", "HospitalCampusInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "park", ["ppi-scale", ["ref", "ParkInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "pedestrian", ["ppi-scale", ["ref", "PedestrianAreaInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    ["aerodrome", "airfield"], ["ppi-scale", ["ref", "AirportAreaInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "harbour", ["ppi-scale", ["ref", "HarborAreaInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "industrial", ["ppi-scale", ["ref", "IndustrialComplexInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "parking", 0,
                    "glacier", ["ppi-scale", ["ref", "LandCover.Glacier.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "water_park", ["ppi-scale", ["ref", "WaterParkInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "railway", ["ppi-scale", ["ref", "RailyardInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "golf_course", ["ppi-scale", ["ref", "GolfCourseInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "reservation", ["ppi-scale", ["ref", "NativeReservationInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "military", ["ppi-scale", ["ref", "MilitaryBaseInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "allotments", ["ppi-scale", ["ref", "AllotmentInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "sports_centre", ["ppi-scale", ["ref", "SportsComplexInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "commercial", ["ppi-scale", ["ref", "ShoppingComplexInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "beach", ["ppi-scale", ["ref", "BeachInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    "university", ["ppi-scale", ["ref", "UniversityCampusInfo.FontStyle.Size"], ["ref", "ScalePpi.labels"]],
                    ["ppi-scale", 12, ["ref", "ScalePpi.labels"]]],
                "font": ["match", ["get", "kind"],
                    ["national_park", "nature_reserve"], ["ref", "NationalParkInfo.Font"],
                    ["ref", "defaultFont"]],
                "shape": "point"
            },
            "description": "Landuse"
        },
        {
            "id": "landuse_polygon",
            "styleSet": "tilezen_polygons",
            "when": ["==", ["get", "$layer"], "landuse"],
            "technique": "polygon",
            "category": ["match", ["get", "kind"],
                "low_emission_zone", "low_emission_zone",
                ["national_park", "nature_reserve"], "national_park",
                "forest", "forest",
                "theme_park", "theme_park",
                "zoo", "zoo",
                "cemetery", "cemetery",
                "hospital", "hospital",
                "pedestrian", "pedestrian",
                "park", "park",
                "runway", "runway",
                ["aerodrome", "airfield"], "airfield",
                "harbour", "harbour",
                "industrial", "industrial",
                "parking", "parking",
                "glacier", "glacier",
                "water_park", "water_park",
                "railway", "railway",
                "golf_course", "golf_course",
                "reservation", "reservation",
                "military", "military",
                "urban", "urban",
                "urban_area", "urban_area",
                "allotments", "allotments",
                "sports_centre", "sports_centre",
                "commercial", "commercial",
                "beach", "beach",
                "university", "university",
                "landuse"],
            "attr": {
                "disable-shading": ["ref", "Polygon.Land.Disable.Shading"],
                "opacity": ["ref", "globalPolygonOpacity"],
                "ambient": ["ref", "Polygon.Ambient.Factor"],
                "softdepth": ["ref", "softDepthThreshold"],
                "color": ["match", ["get", "kind"],
                    "low_emission_zone", ["ref", "CongestionZone.Color"],
                    ["national_park", "nature_reserve"], ["ref", "NationalPark.Color"],
                    "forest", ["ref", "Woodland.Color"],
                    "theme_park", ["ref", "AmusementPark.Color"],
                    "zoo", ["ref", "AnimalPark.Color"],
                    "cemetery", ["ref", "Cemetery.Color"],
                    "hospital", ["ref", "HospitalCampus.Color"],
                    "pedestrian", ["ref", "PedestrianArea.Color"],
                    "park", ["ref", "Park.Color"],
                    "runway", ["ref", "Runway.Color"],
                    ["aerodrome", "airfield"], ["ref", "AirportArea.Color"],
                    "harbour", ["ref", "HarborArea.Color"],
                    "industrial", ["ref", "IndustrialComplex.Color"],
                    "parking", ["ref", "ParkingArea.Color"],
                    "glacier", ["ref", "LandCover.Glacier.Color"],
                    "water_park", ["ref", "WaterPark.Color"],
                    "railway", ["ref", "Railyard.Color"],
                    "golf_course", ["ref", "GolfCourse.Color"],
                    "reservation", ["ref", "NativeReservation.Color"],
                    "military", ["ref", "MilitaryBase.Color"],
                    ["urban", "urban_area"], ["ref", "BuiltupArea.Color"],
                    "allotments", ["ref", "Allotment.Color"],
                    "sports_centre", ["ref", "SportsComplex.Color"],
                    "commercial", ["ref", "ShoppingComplex.Color"],
                    "beach", ["ref", "Beach.Color"],
                    "university", ["ref", "UniversityCampus.Color"],
                    ["ref", "Land.Color"]],
                "diffuse": ["ref", "Polygon.Diffuse.Factor"]
            },
            "description": "Landuse"
        }
    ]
}